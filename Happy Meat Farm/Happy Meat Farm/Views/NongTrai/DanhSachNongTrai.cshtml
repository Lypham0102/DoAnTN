@model List<NongTrai>

<h1>Danh sách nông trại đã duyệt</h1>

@if (Model.Count == 0)
{
    <p>Không tìm thấy nông trại nào.</p>
}
else
{
    <table>
        <thead>
            <tr>
                <th>Tên nông trại</th>
                <th>Địa chỉ</th>
                <th>Số chương nuôi</th>
                <th>Tên vật nuôi</th>
                <th>Diện tích</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var nongTrai in Model)
            {
                <tr>
                    <td>@nongTrai.TenNongTrai</td>
                    <td>@nongTrai.DiaChi</td>
                    <td>@nongTrai.SoChuongNuoi</td>
                    <td>@nongTrai.TenVatNuoi</td>
                    <td>@nongTrai.DienTich</td>
                </tr>
            }
        </tbody>
    </table>
}
