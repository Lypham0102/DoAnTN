@model Happy_Meat_Farm.Models.NhanVien

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Chi tiết</h1>

<div>
    <h4>Thông tin nhân viên</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model._id)
        </dd>
        <dt class="col-sm-2">
            Mã nhân viên:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MaNV)
        </dd>
        <dt class="col-sm-2">
            Tên nhân viên:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TenNV)
        </dd>
        <dt class="col-sm-2">
            CCCD:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CCCD)
        </dd>
        <dt class="col-sm-2">
            SĐT:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SDT)
        </dd>
        <dt class="col-sm-2">
            Địa chỉ:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DiaChi)
        </dd>
        <dt class="col-sm-2">
            Tên tài khoản:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TenTaiKhoan)
        </dd>
        <dt class="col-sm-2">
            Mật khẩu:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Passwork)
        </dd>
    </dl>
</div>
