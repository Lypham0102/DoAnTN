@model Happy_Meat_Farm.Models.ThucAnModel

@{
    ViewData["Title"] = "Chi tiết";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Thông tin thức ăn</h1>
<dl class="row">
    <dt class="col-sm-2">
        ID:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model._id)
    </dd>
    <dt class="col-sm-2">
        Tên Hãng:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.TenHang)
    </dd>
    <dt class="col-sm-2">
        Mã loại:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.MaLoai)
    </dd>
    <dt class="col-sm-2">
        Khối lượng tĩnh:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.KhoiLuongTinh)
    </dd>
    <dt class="col-sm-2">
        Đối tượng:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.DoiTuong)
    </dd>
    <dt class="col-sm-2">
        Hạn sử dụng:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.HanDung)
    </dd>
    <dt class="col-sm-2">
        Đơn giá:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.DonGia)
    </dd>
    <dt class="col-sm-2">
        ID Phiếu nhập:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.SoLuong)
    </dd>
    <dt class="col-sm-2">
        Thuộc Nông Trại:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model._idNongTrai)
    </dd>
    <dt class="col-sm-2">
        ID Kiểm kê:
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model._idKiemKe)
    </dd>
   
</dl>
