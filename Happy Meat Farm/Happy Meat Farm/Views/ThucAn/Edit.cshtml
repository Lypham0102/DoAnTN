@model Happy_Meat_Farm.Models.ThucAnModel

@{
    ViewData["Title"] = "Chỉnh sửa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Thêm mới thức ăn</h2>
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditPost">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="_id" class="control-label">ID</label>
                <input asp-for="_id" class="form-control" />
                <span asp-validation-for="_id" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for=" TenHang" class=""> Tên hãng  </label>
                <input asp-for=" TenHang" type="text" class="form-control" />
                <span asp-validation-for="TenHang" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MaLoai" class=""> Mã loại  </label>
                <input asp-for="MaLoai" class="form-control" />
                <span asp-validation-for="MaLoai" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="KhoiLuongTinh" class=""> Khối lượng tĩnh </label>
                <input asp-for="KhoiLuongTinh" class="form-control" />
                <span asp-validation-for="KhoiLuongTinh" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DoiTuong" class=""> Đối Tượng  </label>
                <input asp-for="DoiTuong" class="form-control" />
                <span asp-validation-for="DoiTuong" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for=" DonGia" class=""> Đơn Giá </label>
                <input asp-for="DonGia" class="form-control" />
                <span asp-validation-for="DonGia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HanDung" class=""> Hạn Sử Dụng </label>
                <input asp-for="HanDung" type="date" class="form-control" />
                <span asp-validation-for="HanDung" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SoLuong" class=""> Số lượng </label>
                <input asp-for="SoLuong" class="form-control" />
                <span asp-validation-for="SoLuong" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="_idNongTrai" class=""> ID Nông Trại </label>
                <input asp-for="_idNongTrai" class="form-control" />
                <span asp-validation-for="_idNongTrai" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="_idKiemKe" class=""> ID Kiểm Kê </label>
                <input asp-for="_idKiemKe" class="form-control" />
                <span asp-validation-for="_idKiemKe" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Lưu" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
@*<button class="btn btn-primary"><a class="text-white" asp-action="Index">Trở về danh sách</a></button>*@




